              </div>
              <div class="form-group">
                <!-- <label for="bio">Task Description</label> -->
                <textarea class="form-control" type="text" name="description" id="description" rows="5" value="" placeholder="Description" required></textarea>
              </div>
            </div>
            <div class="modal-footer border-top-0 d-flex justify-content-center">
              <button type="submit" class="btn btn-outline-success">Add item</button>
            </div>
          </form>
        </div>
